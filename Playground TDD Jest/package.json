{
	"name": "playground",
	"version": "1.0.0",
	"description": "Playground para practicar TDD con Jest",
	"author": "Oscar Valenzuela",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"test": "jest --verbose --forceExit --maxWorkers=1",
		"test:watchAll": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"devel": "nodemon ./index.js",
		"start": "node ./index.js",
		"devel:json-server": "json-server db/db.json -p 4000"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"express": "^4.17.1",
		"jest": "^26.6.3",
		"json-server": "^0.16.3",
		"nodemon": "^2.0.6",
		"supertest": "^6.0.1"
	}
}
